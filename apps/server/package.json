{
  "name": "server",
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "build": "bun build --compile --minify --sourcemap --outfile server src/index.ts",
    "start": "bun run src/index.ts",
    "check": "bunx ultracite fix --unsafe",
    "typecheck": "tsgo --noEmit",
    "auth:generate": "bunx @better-auth/cli@latest generate",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@elysiajs/cors": "^1.4.0",
    "@phase/shared": "workspace:*",
    "@react-email/components": "^1.0.1",
    "@react-email/render": "^2.0.0",
    "better-auth": "1.4.2",
    "drizzle-orm": "^0.44.7",
    "elysia": "^1.4.19",
    "ioredis": "^5.4.2",
    "maxmind": "^5.0.1",
    "nanoid": "^5.1.6",
    "pg": "^8.16.3",
    "react": "^19.2.3",
    "ulid": "^3.0.1"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.5",
    "@types/bun": "latest",
    "@types/pg": "^8.15.6",
    "@types/react": "^19.1.8",
    "@typescript/native-preview": "^7.0.0-dev.20251203.1",
    "bun-types": "latest",
    "drizzle-kit": "^0.31.6",
    "typescript": "^5",
    "ultracite": "5.6.1"
  },
  "module": "src/index.js"
}
